{
  "name": "clonegoat-backend",
  "version": "1.0.0",
  "description": "Backend for CloneGOAT project",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node-pg-migrate --migrations-dir src/database/migrations",
    "migrate:up": "node-pg-migrate up --migrations-dir src/database/migrations",
    "migrate:down": "node-pg-migrate down --migrations-dir src/database/migrations",
    "migrate:create": "node-pg-migrate create --migrations-dir src/database/migrations"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mongoose": "^5.10.9",
    "node-pg-migrate": "^7.9.1",
    "pg": "^8.13.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  },
  "author": "Your Name",
  "license": "MIT"
}
